# Environment configuration for Solana Whale Watcher 2.0
# Copy this file to `.env` and adjust values for your setup.

# Helius RPC endpoint (sample: https://mainnet.helius-rpc.com/?api-key=YOUR_KEY)
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_RPC_KEY

# Helius REST / AI API key (bearer token for analytics endpoints)
HELIUS_API_KEY=YOUR_HELIUS_REST_API_KEY

# Optional overrides
DEFAULT_TOKEN_MINT=So11111111111111111111111111111111111111112
MAX_POSITION_PERCENT=0.02
STOP_LOSS_PERCENT=0.08
TAKE_PARTIAL_PERCENT=1.0
DEFAULT_BANKROLL=5000
